{
    "name": "preWiki",
    "version": "1.0",
    "manifest_version": 2,
    "icons": { "16": "icon16.png",
  	       "48": "icon48.png",
 	      "128": "icon128.png" },
    "content_scripts": [{
        "js": ["contentscript.js"],
        "matches": ["https://*.wikipedia.org/wiki/*"]
    }],
    "web_accessible_resources": ["script.js"]
}
